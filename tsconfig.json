{
    "compilerOptions": {
      "target": "es2022",
      "module": "commonjs",
      "baseUrl": "./",
      "paths": {
        "@/*": ["./src/*"],
        "test/*": ["./test/*"]
      },
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "types": [
        "vitest/globals"
      ]
    }
  }
  